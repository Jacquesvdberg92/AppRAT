@model AppRat.Models.ARL_Auth

@{
    ViewData["Title"] = "Create";
}
<div class="row">
    <form asp-action="Create">
        <div class="data-panel">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="d-flex justify-content-between align-items-center mb-3">
                <h3 class="mb-0">Create Authentications Level</h3>
                <div>
                    <input type="submit" value="Create" class="btn btn-outline-theme p-1" />
                    <a asp-action="Index" class="btn btn-outline-theme p-1">Back to List</a>
                </div>
            </div>
            <hr />
            <div class="row">
                <div class="col-12">
                    <div class="overflow-container">
                        <table>
                            <tbody>
                                <tr>
                                    <label asp-for="Description" class="control-label"></label>
                                </tr>
                                <tr>
                                    <input asp-for="Description" class="form-control" />
                                    <span asp-validation-for="Description" class="text-danger"></span>
                                </tr>
                            </tbody>
                        </table>                          
                    </div>
                </div>
            </div>

        </div>
    </form>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
