@model AppRat.Models.AR_Target

@{
    ViewData["Title"] = "Details";
}

<div class="row">
    <div class="data-panel">
        <div class="row">
            <div class="col-12 d-flex justify-content-between align-items-center mb-3">
                <h1>Target Details</h1>
                <div>
                    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-outline-theme p-1">Edit</a>
                    <a asp-action="Index" class="btn btn-outline-theme p-1">Back to List</a>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.UserId)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.UserId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.New)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.New)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Used)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Used)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.Date)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.Date)
                    </dd>
                </dl>
            </div>
        </div>
    </div>
</div>
