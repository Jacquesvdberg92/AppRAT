@model AppRat.Models.ARR_DealerLink

@{
    ViewData["Title"] = "Create";
}

<div class="row">
    <form asp-action="Create">
        <div class="data-panel">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="d-flex justify-content-between align-items-center mb-3">
                <h3 class="mb-0">Create Dealer Link</h3>
                <div>
                    <input type="submit" value="Create" class="btn btn-outline-theme p-1" />
                    <a asp-action="Index" class="btn btn-outline-theme p-1">Back to List</a>
                </div>
            </div>
            <hr />
            <div class="row">
                <div class="col-12">
                    <div class="overflow-container">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group p-1">
                            <label asp-for="UserId" class="control-label">User</label>
                            <label>User</label>
                            <select asp-for="UserId" class="form-select" asp-items="ViewBag.UserId"></select>
                            <span asp-validation-for="UserId" class="text-danger"></span>
                        </div>
                        <div class="form-group p-1">
                            <label asp-for="DealerId" class="control-label"></label>
                            <select asp-for="DealerId" class="form-select" asp-items="ViewBag.DealerId"></select>
                            <span asp-validation-for="DealerId" class="text-danger"></span>
                        </div>
                        
                    </div>
                </div>
            </div>

        </div>
    </form>
</div>

